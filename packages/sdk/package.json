{
  "name": "@merlion/sdk",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "protoc": "protoc --ts_out src/proto --ts_opt long_type_string --proto_path $(PWD)/proto $(find $(PWD)/proto -iname \"*.proto\")",
